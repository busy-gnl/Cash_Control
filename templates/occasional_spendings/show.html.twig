{% extends 'base_dashboard.html.twig' %}

{% block title %}OccasionalSpendings
{% endblock %}

{% block body %}
	<main class="container mx-5">
		<h1>OccasionalSpendings</h1>

		<table class="table">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ occasional_spending.id }}</td>
				</tr>
				<tr>
					<th>Name</th>
					<td>{{ occasional_spending.name }}</td>
				</tr>
				<tr>
					<th>Amount</th>
					<td>{{ occasional_spending.amount }}</td>
				</tr>
			</tbody>
		</table>

		<a href="{{ path('app_occasional_spendings_index') }}">back to list</a>

		<a href="{{ path('app_occasional_spendings_edit', {'id': occasional_spending.id}) }}">edit</a>

		{{ include('occasional_spendings/_delete_form.html.twig') }}
	</main>
{% endblock %}
